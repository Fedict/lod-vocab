  skos:inScheme <http://vocab.belgif.be/auth/gdpr-finality#id>;
  skos:notation "A5_01";
  skos:note "De analyse van concurrenten en eventuele handelspartners."@nl, "Die Analyse von Wettbewerbern und möglichen Handelspartnern."@de,
    "L’analyse de concurrents et de partenaires commerciaux éventuels."@fr, "The analysis of competitors and possible trading partners."@en;
  skos:prefLabel "Renseignements technico-commerciaux"@fr, "Technical-commercial information"@en,
