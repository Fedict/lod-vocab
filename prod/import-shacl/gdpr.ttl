		sh:minCount 1 ;
		sh:maxCount 1 ;
		sh:nodeKind sh:BlankNodeOrIRI ;
		sh:class cv:PublicOrganisation ;
	] ;
	sh:property [
		sh:path cv:hasContactPoint ;
		sh:minCount 1 ;
		sh:nodeKind sh:BlankNodeOrIRI ;
		sh:class schema:ContactPoint ;
	] .
